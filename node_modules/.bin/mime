#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alex/adev/code/myApps/raspisanie/node_modules/.pnpm/mime@2.5.2/node_modules/mime/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules/.pnpm/mime@2.5.2/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules/.pnpm/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules:/home/alex/adev/code/myApps/node_modules:/home/alex/adev/code/node_modules:/home/alex/adev/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules/mime/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/alex/adev/code/myApps/raspisanie/node_modules/.pnpm/mime@2.5.2/node_modules/mime/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules/.pnpm/mime@2.5.2/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules/.pnpm/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules:/home/alex/adev/code/myApps/node_modules:/home/alex/adev/code/node_modules:/home/alex/adev/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/adev/code/myApps/raspisanie/node_modules/mime/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
